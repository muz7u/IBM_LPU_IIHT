<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

<bean name="myCoach" class="comm.example.bean.BaseBallCoach">
<property name="fortuneService" ref="fortuneService"/>
</bean>

<bean name="fortuneService" class="comm.example.bean.DailyFortuneService">
</bean>



<bean name="myDetails" class="comm.example.bean.Name">
<property name="refAddressInterface" ref="addressInterface"/>
</bean>

<bean name="addressInterface" class="comm.example.bean.Address">
</bean>




<bean name="thePerson" class="comm.example.project.Person">
		<property name="address" ref="address"/>
</bean>


<bean name="address" class="comm.example.project.Address">
</bean>




